{
    "manifest_version": 2,
    "name": "LazyYouTubeWatcher",
    "version": "0.1.0",
    "description": "Prevents you from endlessly streaming YouTube videos when you fall asleep.You can contribute: https://github.com/Mohamed-Fdl/lazy-youtube-watcher",
    "homepage_url": "https://github.com/Mohamed-Fdl/lazy-youtube-watcher",
    "icons": {
        "48": "icons/48.png",
        "96": "icons/96.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "options_ui": {
        "page": "options/index.html"
    },
    "permissions": [
        "notifications",
        "activeTab",
        "webRequest",
        "*://*/*",
        "storage"
    ],
    "browser_action": {
        "default_icon": "icons/32.png",
        "default_title": "LazyYouTubeWatcher",
        "default_popup": "popup/popup.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "{23dd3846-ff14-438a-ae5b-ebbada68f5ee}"
        }
    }
}