{
    "manifest_version": 2,
    "name": "LazyYouTubeWatcher",
    "version": "0.1.0",
    "description": "Prevents you from endlessly streaming YouTube videos when you fall asleep.",
    "icons": {
        "48": "icons/48.png",
        "96": "icons/96.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "main.js"
            ]
        }
    ]
}